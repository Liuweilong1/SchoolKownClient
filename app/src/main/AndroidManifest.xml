<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schoolkownclient">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Shop.ConfirmActivity"></activity>
        <activity android:name=".Activity.Shop.HistoryItem" />
        <activity android:name=".Activity.ParentMainActivity" />
        <activity android:name=".Activity.MyClassActivity.ChatActivity" />
        <activity android:name=".Activity.MyClassActivity.MemberActivity" />
        <activity android:name=".Activity.LoginActivity.RegisterActivity" />
        <activity android:name=".Activity.LoginActivity.ForgetPwdActivity" />
        <activity android:name=".Activity.LoginActivity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
    </application>

</manifest>